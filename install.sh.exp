#!/usr/bin/env expect

spawn ./install.sh
expect  "do you want to always run the latest version of hosty? (recommended) y/n\r"
send -- "y\r"
expect "do you want to automatically update your hosts file with the latest ads list? (recommended) y/n\r"
send -- "y\r"
expect "enter 'daily', 'weekly', 'monthly' or 'never':\r"
send -- "daily\r"
expect eof
